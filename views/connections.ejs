<!DOCTYPE html>
<html lang="en">

    <%- include ('partials/head.ejs') -%>

<body >
 

    <%- include ('partials/header.ejs') -%>
    <%- include ('partials/nav.ejs') -%>
                        
                                              
    <section class="bringtotop">
        <% sessionData.categoryTopics.forEach(function(topic) { %>
          <h1><%= topic %></h1>
          <% sessionData.connections.forEach(function(connection) {
              if (topic === (connection.Topic)) { %>
          <ul>
            <li>
                <a href="/connection?connectionID=<%= connection.workshopId%>">
                    <%= connection.Name %>
                </a>
            </li>
          </ul>
          <% };%>
          <% }); %>
          <% }); %>
        </section>               
        

            <%- include ('partials/footer.ejs') -%>
         
</body>
</html>